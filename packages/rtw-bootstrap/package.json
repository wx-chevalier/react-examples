{
  "name": "rtw-bootstrap",
  "version": "0.0.1",
  "description": "rtw-bootstrap",
  "repository": {
    "type": "git",
    "url": "https://github.com/wx-chevalier/fe-boilerplates"
  },
  "license": "MIT",
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "wx-chevalier@github",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{ts,tsx,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "analyze": "npm run stats && webpack-bundle-analyzer dist/stats.json",
    "clean": "rimraf dist",
    "dev": "webpack-dev-server --config ./scripts/webpack/webpack.config.dev.js --hot",
    "build": "npm run build:prod",
    "build:prod": "NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.prod.js",
    "start": "webpack-dashboard -- npm run dev",
    "stats": "webpack --config ./scripts/webpack.config.prod.js --mode production --profile --json > dist/stats.json"
  },
  "dependencies": {
    "rtw-core": "*",
    "systemjs": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/react-dom": "^16.9.0",
    "@types/systemjs": "^0.20.6",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "autoprefixer": "9.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.19.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "3.2.0",
    "enzyme": "^3.10.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-loader": "3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "4.2.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "json-server": "0.15.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "parallelshell": "^3.0.2",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "^4.12.11",
    "resolve-url-loader": "3.1.0",
    "rimraf": "^3.0.0",
    "style-loader": "1.0.0",
    "systemjs": "^5.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "6.0.4",
    "typescript": "3.5.3",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "^2.1.0",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.39.2",
    "webpack-cli": "3.3.7",
    "webpack-dashboard": "3.0.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1",
    "workerize-loader": "^1.1.0"
  },
  "files": [
    "dist/"
  ]
}
